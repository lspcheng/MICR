  ],
  "keyChoiceText": ["<div class=\"vertical-center\" style='width:60%;'><div style='float: left;'><p>Press '1' for same.</p></div><div style='float: right;'><p>Press '0' for different.</p></div></div>"
  ],
  "sliderPromptText": ["<div class=\"vertical-center\"><p>On a scale from 1 (very different) to 7 (very similar), how similar were the two pronunciations?</p></div><br>"
